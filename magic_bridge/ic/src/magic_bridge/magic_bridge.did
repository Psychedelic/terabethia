  CreateCanisterError;
  EncodeError;
  CodeAlreadyInstalled;
  InstallCodeError;
};

type TokenType = variant {
  DIP20;
  DIP721;
};

type Result = variant { Ok : principal; Err : FactoryError };
service : (principal) -> {
  create : (
      text,
      text,
      text,
      nat8,
      nat,
      principal,
      vec principal,
      nat64,
      nat,
      principal,
      principal,
      TokenType,
    ) -> (Result);
  owner : () -> (principal) query;
}
