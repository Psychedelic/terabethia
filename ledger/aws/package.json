{
  "name": "ledger",
  "version": "0.0.1",
  "description": "Ledger Centerlaized Infra",
  "scripts": {
    "dev": "sls offline start",
    "local": "sls offline start --stage local",
    "typecheck": "tsc --noEmit",
    "deploy:dev": "sls deploy --stage dev",
    "deploy:prod": "sls deploy --stage prod -v",
    "build": "rm -rf dist && tsc -b tsconfig.json",
    "format": "prettier --write \"./src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"./src/**/*.{ts,tsx,js,jsx}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --fix --ext ts && prettier --write \"./src/**/*.{ts,tsx,js,jsx}\"",
    "script-local": "ts-node"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
